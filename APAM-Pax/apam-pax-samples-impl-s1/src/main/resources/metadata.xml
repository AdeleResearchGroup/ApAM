		<property name="hasDisplay" value="false" />
	</implementation>

	<implementation name="BoschSwitch"
		classname="fr.imag.adele.apam.pax.test.impl.device.GenericSwitch"
		specification="eletronic-device">
		<property name="manufacturer" value="bosch" />
